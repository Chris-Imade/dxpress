  <style>
    :root {
      --primary: #4f46e5;
      --primary-dark: #4338ca;
      --primary-light: #e0e7ff;
      --secondary: #06b6d4;
      --secondary-dark: #0891b2;
      --secondary-light: #cffafe;
      --dark: #1e293b;
      --gray-light: #f8fafc;
      --gray: #e2e8f0;
      --white: #ffffff;
      --text: #334155;
      --text-light: #64748b;
    }

    * {
      margin: 0;
      padding: 0;
      box-sizing: border-box;
    }

    body {
      font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;
      color: var(--text);
      line-height: 1.6;
      background-color: var(--gray-light);
    }

    a {
      text-decoration: none;
      color: inherit;
    }

    .container {
      width: 100%;
      max-width: 1200px;
      margin: 0 auto;
      padding: 0 1.5rem;
    }

    .btn {
      display: inline-block;
      padding: 0.8rem 1.5rem;
      border-radius: 0.5rem;
      font-weight: 600;
      cursor: pointer;
      transition: all 0.3s ease;
      text-align: center;
    }

    .btn-primary {
      background-color: var(--primary);
      color: var(--white);
    }

    .btn-primary:hover {
      background-color: var(--primary-dark);
      transform: translateY(-2px);
      box-shadow: 0 4px 12px rgba(79, 70, 229, 0.2);
    }

    .btn-secondary {
      background-color: transparent;
      border: 1px solid var(--white);
      color: var(--white);
    }

    .btn-secondary:hover {
      background-color: rgba(255, 255, 255, 0.1);
      transform: translateY(-2px);
    }

    .btn-dark {
      background-color: var(--secondary);
      color: var(--white);
    }

    .btn-dark:hover {
      background-color: var(--secondary-dark);
      transform: translateY(-2px);
      box-shadow: 0 4px 12px rgba(6, 182, 212, 0.2);
    }

    .btn-outline {
      background-color: transparent;
      border: 1px solid var(--primary);
      color: var(--primary);
    }

    .btn-outline:hover {
      background-color: rgba(79, 70, 229, 0.05);
      transform: translateY(-2px);
    }

    /* Navigation */
    .navbar {
      display: flex;
      justify-content: space-between;
      align-items: center;
      padding: 1.5rem 0;
    }

    .logo {
      font-size: 1.5rem;
      font-weight: 700;
      color: var(--primary);
      display: flex;
      align-items: center;
    }

    .logo i {
      margin-right: 0.5rem;
    }

    .nav-links {
      display: flex;
      gap: 2rem;
      list-style: none;
    }

    .nav-links li a {
      font-weight: 500;
      transition: color 0.3s ease;
    }

    .nav-links li a:hover {
      color: var(--primary);
    }

    /* Hero section */
    .hero {
      background-image: linear-gradient(rgba(0, 0, 0, 0.5), rgba(0, 0, 0, 0.5)), url("/assets/images/thumbs/ecommerce.webp");
      background-size: cover;
      background-position: center;
      color: var(--white);
      padding: 6rem 0;
      position: relative;
      overflow: hidden;
    }

    .hero::after {
      content: '';
      position: absolute;
      right: -5%;
      top: -5%;
      width: 500px;
      height: 500px;
      background: radial-gradient(circle, rgba(255, 255, 255, 0.1) 0%, rgba(255, 255, 255, 0) 70%);
      border-radius: 50%;
      z-index: 1;
    }

    .hero::before {
      content: '';
      position: absolute;
      left: -10%;
      bottom: -30%;
      width: 600px;
      height: 600px;
      background: radial-gradient(circle, rgba(255, 255, 255, 0.08) 0%, rgba(255, 255, 255, 0) 70%);
      border-radius: 50%;
      z-index: 1;
    }

    .hero-content {
      position: relative;
      z-index: 2;
      max-width: 800px;
      margin: 0 auto;
      text-align: center;
    }

    .hero-badge {
      display: inline-block;
      background-color: rgba(255, 255, 255, 0.1);
      padding: 0.5rem 1rem;
      border-radius: 2rem;
      font-size: 0.9rem;
      font-weight: 500;
      margin-bottom: 1.5rem;
      backdrop-filter: blur(10px);
    }

    .hero-title {
      font-size: 3.5rem;
      font-weight: 800;
      line-height: 1.2;
      margin-bottom: 1.5rem;
      color: white;
    }

    .hero-description {
      font-size: 1.2rem;
      margin-bottom: 2rem;
      opacity: 0.9;
      max-width: 600px;
      margin-left: auto;
      margin-right: auto;
    }

    .hero-btns {
      display: flex;
      gap: 1rem;
      justify-content: center;
      flex-wrap: wrap;
    }

    /* Features section */
    .section {
      padding: 5rem 0;
    }

    .section-header {
      text-align: center;
      margin-bottom: 4rem;
    }

    .section-badge {
      display: inline-block;
      background-color: var(--primary-light);
      color: var(--primary);
      padding: 0.5rem 1rem;
      border-radius: 2rem;
      font-size: 0.9rem;
      font-weight: 500;
      margin-bottom: 1rem;
    }

    .section-title {
      font-size: 2.5rem;
      font-weight: 700;
      margin-bottom: 1rem;
      color: var(--dark);
    }

    .section-description {
      font-size: 1.1rem;
      color: var(--text-light);
      max-width: 600px;
      margin: 0 auto;
    }

    .features-grid {
      display: grid;
      grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
      gap: 2rem;
    }

    .feature-card {
      background-color: var(--white);
      border-radius: 1rem;
      padding: 2rem;
      box-shadow: 0 4px 20px rgba(0, 0, 0, 0.05);
      transition: transform 0.3s ease, box-shadow 0.3s ease;
      border: 1px solid var(--gray);
    }

    .feature-card:hover {
      transform: translateY(-5px);
      box-shadow: 0 8px 30px rgba(0, 0, 0, 0.08);
      border-color: var(--primary-light);
    }

    .feature-icon {
      display: inline-flex;
      align-items: center;
      justify-content: center;
      width: 3rem;
      height: 3rem;
      border-radius: 0.8rem;
      background-color: var(--primary-light);
      color: var(--primary);
      font-size: 1.2rem;
      margin-bottom: 1.5rem;
    }

    .feature-title {
      font-size: 1.3rem;
      font-weight: 600;
      margin-bottom: 1rem;
      color: var(--dark);
    }

    .feature-description {
      color: var(--text-light);
      line-height: 1.6;
    }

    /* Platform section */
    .platforms {
      background-color: var(--white);
    }

    .platforms-grid {
      display: grid;
      grid-template-columns: repeat(auto-fit, minmax(150px, 1fr));
      gap: 2rem;
      margin-bottom: 3rem;
    }

    .platform-card {
      height: 100px;
      background-color: var(--gray-light);
      border-radius: 1rem;
      display: flex;
      align-items: center;
      justify-content: center;
      border: 1px solid var(--gray);
      padding: 1.5rem;
      transition: all 0.3s ease;
    }

    .platform-card:hover {
      transform: scale(1.05);
      box-shadow: 0 8px 20px rgba(0, 0, 0, 0.05);
      border-color: var(--primary-light);
    }

    .platform-card img {
      max-width: 100%;
      max-height: 60px;
      filter: grayscale(100%);
      opacity: 0.7;
      transition: all 0.3s ease;
    }

    .platform-card:hover img {
      filter: grayscale(0%);
      opacity: 1;
    }

    .api-section {
      background-color: var(--gray-light);
      border-radius: 1rem;
      padding: 2rem;
      display: flex;
      flex-wrap: wrap;
      gap: 3rem;
      align-items: center;
      border: 1px solid var(--gray);
    }

    .api-content {
      flex: 1;
      min-width: 300px;
    }

    .api-badge {
      font-size: 0.9rem;
      color: var(--primary);
      font-weight: 600;
      margin-bottom: 0.5rem;
      display: inline-block;
    }

    .api-code {
      flex: 1;
      min-width: 300px;
    }

    .code-container {
      background-color: var(--dark);
      border-radius: 0.8rem;
      padding: 1.5rem;
      box-shadow: 0 4px 15px rgba(0, 0, 0, 0.1);
    }

    .code-header {
      color: var(--text-light);
      font-size: 0.8rem;
      margin-bottom: 0.8rem;
      font-family: monospace;
    }

    .code-block {
      background-color: #1a1a1a;
      border-radius: 0.5rem;
      padding: 1.2rem;
      font-family: monospace;
      font-size: 0.9rem;
      color: #10b981;
      overflow-x: auto;
      line-height: 1.5;
    }

    /* Steps section */
    .steps-container {
      position: relative;
      margin-top: 4rem;
    }

    .steps-line {
      position: absolute;
      top: 3rem;
      left: 10%;
      right: 10%;
      height: 2px;
      background-color: var(--primary-light);
      z-index: 1;
      display: none;
    }

    .steps-grid {
      display: grid;
      grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
      gap: 2rem;
      position: relative;
      z-index: 2;
    }

    .step-card {
      background-color: var(--white);
      border-radius: 1rem;
      padding: 2rem;
      text-align: center;
      box-shadow: 0 4px 20px rgba(0, 0, 0, 0.05);
      border: 1px solid var(--gray);
      position: relative;
      transition: transform 0.3s ease, box-shadow 0.3s ease;
    }

    .step-card:hover {
      transform: translateY(-5px);
      box-shadow: 0 8px 30px rgba(0, 0, 0, 0.1);
      border-color: var(--primary-light);
    }

    .step-number {
      width: 3.5rem;
      height: 3.5rem;
      background-color: var(--primary);
      color: var(--white);
      border-radius: 50%;
      display: flex;
      align-items: center;
      justify-content: center;
      font-size: 1.5rem;
      font-weight: 700;
      margin: -4rem auto 2rem;
      box-shadow: 0 4px 12px rgba(79, 70, 229, 0.3);
      border: 4px solid var(--white);
    }

    .step-title {
      font-size: 1.3rem;
      font-weight: 600;
      margin-bottom: 1rem;
      color: var(--dark);
    }

    .step-description {
      color: var(--text-light);
      margin-bottom: 1.5rem;
    }

    .step-features {
      display: flex;
      flex-direction: column;
      gap: 0.8rem;
      text-align: left;
    }

    .step-feature {
      display: flex;
      align-items: center;
      gap: 0.5rem;
    }

    .step-feature i {
      color: #22c55e;
      font-size: 0.9rem;
    }

    .help-banner {
      display: inline-flex;
      align-items: center;
      background-color: var(--primary-light);
      padding: 0.8rem 1.5rem;
      border-radius: 2rem;
      margin-top: 3rem;
      color: var(--text);
      font-size: 0.9rem;
    }

    .help-banner i {
      color: var(--primary);
      margin-right: 0.5rem;
    }

    /* CTA section */
    .cta {
      background: linear-gradient(135deg, var(--secondary) 0%, var(--secondary-dark) 100%);
      color: var(--white);
      padding: 6rem 0;
      position: relative;
      overflow: hidden;
    }

    .cta::after {
      content: '';
      position: absolute;
      right: -10%;
      top: -10%;
      width: 500px;
      height: 500px;
      background: radial-gradient(circle, rgba(255, 255, 255, 0.1) 0%, rgba(255, 255, 255, 0) 70%);
      border-radius: 50%;
      z-index: 1;
    }

    .cta-content {
      position: relative;
      z-index: 2;
      max-width: 800px;
      margin: 0 auto;
      text-align: center;
    }

    /* Footer */
    footer {
      background-color: var(--dark);
      color: var(--white);
      padding: 4rem 0 2rem;
    }

    .footer-content {
      display: flex;
      flex-wrap: wrap;
      gap: 3rem;
      margin-bottom: 3rem;
    }

    .footer-logo {
      flex: 1;
      min-width: 200px;
    }

    .footer-logo-text {
      font-size: 1.5rem;
      font-weight: 700;
      margin-bottom: 1rem;
      display: inline-flex;
      align-items: center;
    }

    .footer-logo-text i {
      margin-right: 0.5rem;
    }

    .footer-description {
      color: var(--text-light);
      max-width: 300px;
    }

    .footer-links {
      flex: 1;
      min-width: 200px;
    }

    .footer-title {
      font-size: 1.1rem;
      font-weight: 600;
      margin-bottom: 1.5rem;
      color: var(--white);
    }

    .footer-list {
      list-style: none;
      display: flex;
      flex-direction: column;
      gap: 0.8rem;
    }

    .footer-list li a {
      color: var(--text-light);
      transition: color 0.3s ease;
    }

    .footer-list li a:hover {
      color: var(--primary-light);
    }

    .footer-bottom {
      border-top: 1px solid rgba(255, 255, 255, 0.1);
      padding-top: 2rem;
      text-align: center;
      color: var(--text-light);
      font-size: 0.9rem;
    }
    
    .social-icons {
      display: flex;
      gap: 1rem;
      margin-top: 1rem;
    }
    
    .social-icon {
      display: inline-flex;
      align-items: center;
      justify-content: center;
      width: 2.2rem;
      height: 2.2rem;
      border-radius: 50%;
      background-color: rgba(255, 255, 255, 0.1);
      color: var(--white);
      transition: all 0.3s ease;
    }
    
    .social-icon:hover {
      background-color: var(--primary);
      transform: translateY(-3px);
    }

    /* Responsive */
    @media (max-width: 768px) {
      .hero-title {
        font-size: 2.5rem;
      }
      
      .section-title {
        font-size: 2rem;
      }
      
      .hero-btns {
        flex-direction: column;
        width: 100%;
        max-width: 300px;
        margin: 0 auto;
      }
      
      .api-section {
        flex-direction: column;
        gap: 2rem;
      }
      
      .nav-links {
        display: none;
      }
      
      .mobile-menu-btn {
        display: block;
      }
    }

    /* Animations */
    @keyframes fadeInUp {
      from {
        opacity: 0;
        transform: translateY(20px);
      }
      to {
        opacity: 1;
        transform: translateY(0);
      }
    }

    .animate {
      animation: fadeInUp 0.6s ease forwards;
    }

    /* Utility classes */
    .mt-1 { margin-top: 0.5rem; }
    .mt-2 { margin-top: 1rem; }
    .mt-3 { margin-top: 1.5rem; }
    .mt-4 { margin-top: 2rem; }
    
    .check-mark {
      display: inline-flex;
      align-items: center;
      gap: 0.5rem;
    }
    
    .check-mark i {
      color: #22c55e;
    }
    
    /* Mobile menu */
    .mobile-menu-btn {
      display: none;
      cursor: pointer;
      font-size: 1.5rem;
    }
    
    @media (min-width: 769px) {
      .steps-line {
        display: block;
      }
    }

    .d-flex.align-items-center.tw-gap-405.cursor-small.tw-ps-8.line-two {
      display: none !important;
    }
  </style>
</head>
<body>
  <!-- Navigation -->
 

  <!-- Hero Section -->
  <section class="hero">
    <div class="container">
      <div class="hero-content animate">
        <span class="hero-badge">E-commerce Integration</span>
        <h1 class="hero-title">Transform Your Shipping Process</h1>
        <p class="hero-description">Connect your online store with DXpress and experience seamless shipping automation</p>
        <div class="hero-btns">
          <a href="#get-started" class="btn btn-primary">Start Integration</a>
          <a href="#features" class="btn btn-secondary">Learn More</a>
        </div>
      </div>
    </div>
  </section>

  <!-- Features Section -->
  <section id="features" class="section">
    <div class="container">
      <div class="section-header animate">
        <span class="section-badge">Features</span>
        <h2 class="section-title">Powerful Integration Features</h2>
        <p class="section-description">Everything you need to streamline your e-commerce shipping process</p>
      </div>

      <div class="features-grid">
        <div class="feature-card animate" style="animation-delay: 0.1s">
          <div class="feature-icon">
            <i class="fas fa-robot"></i>
          </div>
          <h3 class="feature-title">Automated Order Processing</h3>
          <p class="feature-description">Import orders automatically from your store and create shipping labels with a single click, saving hours of manual work.</p>
        </div>

        <div class="feature-card animate" style="animation-delay: 0.2s">
          <div class="feature-icon">
            <i class="fas fa-tachometer-alt"></i>
          </div>
          <h3 class="feature-title">Real-time Shipping Rates</h3>
          <p class="feature-description">Display accurate shipping rates at checkout based on package dimensions, weight, and destination for better customer experiences.</p>
        </div>

        <div class="feature-card animate" style="animation-delay: 0.3s">
          <div class="feature-icon">
            <i class="fas fa-map-marker-alt"></i>
          </div>
          <h3 class="feature-title">Advanced Order Tracking</h3>
          <p class="feature-description">Automatically send tracking information to customers and update order status in your store to improve customer satisfaction.</p>
        </div>

        <div class="feature-card animate" style="animation-delay: 0.4s">
          <div class="feature-icon">
            <i class="fas fa-boxes"></i>
          </div>
          <h3 class="feature-title">Smart Inventory Management</h3>
          <p class="feature-description">Sync inventory levels across multiple sales channels and warehouses in real-time to avoid stockouts and overselling.</p>
        </div>
      </div>
    </div>
  </section>

  <!-- Platforms Section -->
  <section class="section platforms">
    <div class="container">
      <div class="section-header animate">
        <span class="section-badge">Integrations</span>
        <h2 class="section-title">Supported Platforms</h2>
        <p class="section-description">Seamlessly integrate with all your favorite e-commerce platforms</p>
      </div>

      <div class="platforms-grid animate" style="animation-delay: 0.1s">
        <div class="platform-card">
          <img src="/assets/images/ecom/shopify.png" alt="Shopify" />
        </div>
        <div class="platform-card">
          <img src="/assets/images/ecom/woo.png" alt="WooCommerce" />
        </div>
        <div class="platform-card">
          <img src="/assets/images/ecom/magento.png" alt="Magento" />
        </div>
        <div class="platform-card">
          <img src="/assets/images/ecom/bigcommerce.png" alt="BigCommerce" />
        </div>
      </div>

      <div class="api-section animate" style="animation-delay: 0.2s">
        <div class="api-content">
          <span class="api-badge">Custom Integration</span>
          <h3 class="feature-title">Open API for Any Platform</h3>
          <p class="feature-description mt-2">
            Don't see your platform? No problem. Our robust API allows integration with any e-commerce system.
          </p>
          <a href="#" class="btn btn-outline mt-3">
            Learn about our API
            <i class="fas fa-arrow-right ml-2"></i>
          </a>
        </div>
        <div class="api-code">
          <div class="code-container">
            <div class="code-header">Example API Request</div>
            <pre class="code-block">POST /api/v1/orders
{
  "order_id": "1234",
  "shipping_method": "express",
  "destination": {
    "name": "John Doe",
    "address": "123 Main St"
  }
}</pre>
          </div>
        </div>
      </div>
    </div>
  </section>

  <!-- Integration Process -->
  <section class="section">
    <div class="container">
      <div class="section-header animate">
        <span class="section-badge">Easy Setup</span>
        <h2 class="section-title">Simple Integration Process</h2>
        <p class="section-description">Get started in just three easy steps with our intuitive process</p>
      </div>

      <div class="steps-container">
        <div class="steps-line"></div>
        <div class="steps-grid">
          <div class="step-card animate" style="animation-delay: 0.1s">
            <div class="step-number">1</div>
            <h3 class="step-title">Connect Your Store</h3>
            <p class="step-description">Install our plugin or connect via API to your e-commerce platform in minutes.</p>
            <div class="step-features">
              <div class="step-feature">
                <i class="fas fa-check-circle"></i>
                <span>One-click installation</span>
              </div>
              <div class="step-feature">
                <i class="fas fa-check-circle"></i>
                <span>No coding required</span>
              </div>
            </div>
          </div>

          <div class="step-card animate" style="animation-delay: 0.2s">
            <div class="step-number">2</div>
            <h3 class="step-title">Configure Settings</h3>
            <p class="step-description">Set up your shipping preferences, rates, and automation rules through our intuitive dashboard.</p>
            <div class="step-features">
              <div class="step-feature">
                <i class="fas fa-check-circle"></i>
                <span>Custom shipping rules</span>
              </div>
              <div class="step-feature">
                <i class="fas fa-check-circle"></i>
                <span>Flexible pricing options</span>
              </div>
            </div>
          </div>

          <div class="step-card animate" style="animation-delay: 0.3s">
            <div class="step-number">3</div>
            <h3 class="step-title">Start Shipping</h3>
            <p class="step-description">Begin processing orders automatically with DXpress and delight your customers with fast delivery.</p>
            <div class="step-features">
              <div class="step-feature">
                <i class="fas fa-check-circle"></i>
                <span>Automated order processing</span>
              </div>
              <div class="step-feature">
                <i class="fas fa-check-circle"></i>
                <span>Real-time tracking updates</span>
              </div>
            </div>
          </div>
        </div>
      </div>

      <div class="text-center">
        <div class="help-banner animate" style="animation-delay: 0.4s">
          <i class="fas fa-info-circle"></i>
          Need help? Our support team is available to assist with your integration
        </div>
      </div>
    </div>
  </section>

  <!-- CTA Section -->
  <section id="get-started" class="cta">
    <div class="container">
      <div class="cta-content animate">
        <span class="hero-badge">Ready to Get Started?</span>
        <h2 class="section-title" style="color: white">Transform Your Shipping Today</h2>
        <p class="hero-description">Join thousands of successful e-commerce businesses using DXpress to streamline their shipping operations</p>
        <div class="hero-btns mt-4">
          <a href="#" class="btn btn-primary" style="background-color: white; color: var(--secondary)">Get Started Now</a>
          <a href="#" class="btn btn-secondary">Schedule Demo</a>
        </div>
      </div>
    </div>
  </section>
